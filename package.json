{
  "name": "com.codetroopers.website",
  "version": "0.0.1",
  "scripts": {
    "gulp": "gulp",
    "build": "gulp build --gulpfile site/gulpfile.js",
    "build.all": "gulp build --gulpfile site/gulpfile.js && hugo -t code-troopers -s site",
    "build.prod": "gulp build --prod --gulpfile site/gulpfile.js && hugo -t code-troopers -s site && gulp rev --gulpfile site/gulpfile.js",
    "images.compress": "gulp images.compress --gulpfile site/gulpfile.js",
    "images.thumbnail": "gulp images.thumbnail --gulpfile site/gulpfile.js",
    "start": "hugo serve -t code-troopers -w --buildDrafts -s site",
    "clean": "gulp clean --gulpfile site/gulpfile.js",
    "deploy": "gulp ghpages --gulpfile site/gulpfile.js"
  },
  "devDependencies": {
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-clean-css": "^2.0.9",
    "gulp-concat": "^2.6.0",
    "gulp-concat-css": "^2.3.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-if": "^2.0.1",
    "gulp-image-resize": "^0.8.0",
    "gulp-imagemin": "^3.0.1",
    "gulp-less": "^3.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.0.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "run-sequence": "^1.2.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.6.3",
    "jquery": "^2.2.4",
    "jquery.scrollto": "^2.1.2",
    "lightbox2": "^2.8.2",
    "owlcarousel": "^1.3.3"
  }
}
